{
  "schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.3",
  "body": [
      {
          "type": "Container",
          "$data": "${events}",
          "items": [
              {
                  "type": "ColumnSet",
                  "columns": [
                      {
                          "type": "Column",
                          "items": [
                              {
                                  "type": "TextBlock",
                                  "size": "Default",
                                  "text": "${subject}",
                                  "wrap": true,
                                  "weight": "Bolder"
                              },
                              {
                                  "type": "ColumnSet",
                                  "columns": [
                                      {
                                          "type": "Column",
                                          "width": "auto",
                                          "items": [
                                              {
                                                  "type": "Image",
                                                  "url": "data:image/png;base64,",
                                                  "width": "14px",
                                                  "height": "14px"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${location}",
                                                  "horizontalAlignment": "Left",
                                                  "size": "Small",
                                                  "isSubtle": true
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "stretch",
                                          "items": [
                                              {
                                                  "type": "Image",
                                                  "url": "data:image/png;base64,",
                                                  "size": "Small",
                                                  "spacing": "None",
                                                  "width": "14px",
                                                  "height": "14px",
                                                  "horizontalAlignment": "Right"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "auto",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${startTime}",
                                                  "isSubtle": true,
                                                  "size": "Small"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "auto",
                                          "items": [
                                              {
                                                  "type": "Image",
                                                  "url": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAwAAAAMCAYAAABWdVznAAAAAXNSR0IArs4c6QAAAHdJREFUKFNjZMACGhoa2H7+/Gnc3t5+goGB4T+yEkZcGr5//97FwMDworOzsxNZE1YNIENAtmDThFMDLk14NWDTxFheXu7PwMDAjc0vMDFGRka2////FzEwMKwgScP///9Xk+4kfE7BFlLUCVZccQByDc6YxpU0ANRxZM1tjj2pAAAAAElFTkSuQmCC",
                                                  "width": "12px",
                                                  "height": "12px",
                                                  "horizontalAlignment": "Center",
                                                  "spacing": "None"
                                              }
                                          ]
                                      },
                                      {
                                          "type": "Column",
                                          "width": "auto",
                                          "items": [
                                              {
                                                  "type": "TextBlock",
                                                  "text": "${endTime}",
                                                  "size": "Small",
                                                  "isSubtle": true
                                              }
                                          ]
                                      }
                                  ]
                              }
                          ],
                          "width": "stretch"
                      }
                  ],
                  "style": "emphasis",
                  "spacing": "Small",
                  "horizontalAlignment": "Left",
                  "bleed": true,
                  "separator": true
              },
              {
                  "type": "ColumnSet",
                  "columns": [
                      {
                          "type": "Column",
                          "items": [
                              {
                                  "type": "ActionSet",
                                  "actions": [
                                      {
                                          "type": "Action.OpenUrl",
                                          "title": "Ver detalles",
                                          "url": "${url}",
                                          "style": "positive"
                                      }
                                  ],
                                  "spacing": "Small"
                              }
                          ],
                          "width": "auto",
                          "separator": true,
                          "horizontalAlignment": "Center",
                          "verticalContentAlignment": "Top",
                          "spacing": "Small"
                      }
                  ],
                  "horizontalAlignment": "Center"
              }
          ],
          "style": "default",
          "spacing": "Medium",
          "separator": true
      }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json"
}